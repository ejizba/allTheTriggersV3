{
  "bindings": [
    {
      "type": "cosmosDBTrigger",
      "name": "documents",
      "direction": "in",
      "leaseContainerName": "leases",
      "connection": "cosmosDBConnection",
      "databaseName": "helloWorldDB",
      "partitionKey": "/id",
      "containerName": "helloWorldCol",
      "createLeaseContainerIfNotExists": true
    },
    {
      "type": "cosmosDB",
